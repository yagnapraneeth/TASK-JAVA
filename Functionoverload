///driverclass
public class FuncDriver {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		FuncOverload O;
		O=new FuncOverload();
		O.display();
		O.display("yagna");
		O.display("yagna",10);
	}

}



//objectclass

public class FuncOverload {
	void display() {
		System.out.println("no args");
	}
	void display(String name) {
		System.out.println("name is :"+name);
}

void display(String name,int y) {
	System.out.println(" my name is:"+name+ "number is"+y);
}
}

//ouput
no args
name is :yagna
 my name is : yagna number is 10
